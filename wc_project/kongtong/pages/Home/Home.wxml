<swiper indicator-dots="true" autoplay="true" interval="7000" duration="600" circular="true" catchtap='onSwiperTap' style="height: {{clientHeight?clientHeight+'px':''}}">
  <block wx:for="{{imgUrls}}" wx:key="this">
    <swiper-item>
      <image src="{{item.url}}" class="slide-image" id="{{item.id}}"/>
    </swiper-item>
  </block>
</swiper>
<import src="/pages/template.wxml" />
<template is="header"></template>